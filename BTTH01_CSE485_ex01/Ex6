<?php
    $keys = ["field1"=>"first",
             "field2"=>"second",
             "field3"=>"third"];
    $values = ["field1value"=>"dinosaur",
               "field2value"=>"pig",
               "field3value"=>"platypus"];
    $KeysandValues = [];

    foreach($keys as $key1 => $value1){
        foreach($values as $key2 => $value2){
            $KeysandValues[$value1] = $value2;
        }
    };

    foreach($KeysandValues as $key => $value){
        echo "Check {$key} and {$value}". "</br>";
    };
?>
